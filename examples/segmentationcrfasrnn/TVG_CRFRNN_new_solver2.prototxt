net: "TVG_CRFRNN_new_traintest.prototxt"

test_iter: 2
test_interval: 4

display: 2

lr_policy: "fixed"
base_lr: 1e-13
momentum: 0.99

weight_decay: 0.0005

# The maximum number of iterations, DO NOT INCREASE THIS FURTHER
max_iter: 100

# snapshot intermediate results
snapshot: 2
snapshot_prefix: "snapshots/crfrnn" ##CHANGE THIS

# solver mode: CPU or GPU
solver_mode: CPU

